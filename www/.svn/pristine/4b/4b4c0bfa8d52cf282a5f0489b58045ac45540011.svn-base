<ion-header-bar class="background-positive">
	<div class="buttons">
		<button class="button button-icon ion-ios-arrow-back" ng-click="go_state('tab.account')"></button>
	</div>
	<h1 class="title account-title" ng-class="marginleft">设置</h1>
</ion-header-bar>
<ion-view>  
  <ion-content class="settings">    
	<ion-list>
		<ion-item class="item-icon-right" type="item-text-wrap" href="#/tab/change_password">
			<h2>修改密码</h2>
			<i class="icon ion-chevron-right icon-accessory"></i>
		</ion-item>
	</ion-list>
	<ion-list>
		<ion-item class="item-icon-right" type="item-text-wrap" href="#/tab/certification">
			<h2>诚信认证</h2>
			<i class="icon ion-chevron-right icon-accessory"></i>
		</ion-item>
		<ion-item class="item-icon-right" type="item-text-wrap" href="#/tab/online_service">
			<h2>在线客服</h2>
			<i class="icon ion-chevron-right icon-accessory"></i>
		</ion-item>
	</ion-list>
	<ion-list>
		<ion-item class="item-icon-right" type="item-text-wrap" href="#/tab/about_us">
			<h2>关于我们</h2>
			<i class="icon ion-chevron-right icon-accessory"></i>
		</ion-item>
		<ion-item class="item-icon-right" type="item-text-wrap" href="#/tab/privacy_settings">
			<h2>隐私设置</h2>
			<i class="icon ion-chevron-right icon-accessory"></i>
		</ion-item>
	<ion-list>
  </ion-content>
</ion-view>
