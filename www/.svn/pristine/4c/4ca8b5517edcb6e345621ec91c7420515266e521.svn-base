/* Bottom Tab Control */
.bottom-tab  > .tabs{
	padding-top:0;
	height:72px;
	/* FF3.6-15 */
	background: -moz-linear-gradient(top,  rgba(157,213,58,0) 0%, rgba(237,239,228,0) 21%, rgba(243,241,241,1) 22%, rgba(243,241,241,1) 31%, rgba(243,241,241,1) 100%);
	/* Chrome10-25,Safari5.1-6 */
	background: -webkit-linear-gradient(top,  rgba(157,213,58,0) 0%,rgba(237,239,228,0) 21%,rgba(243,241,241,1) 22%,rgba(243,241,241,1) 31%,rgba(243,241,241,1) 100%); 
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	background: linear-gradient(to bottom,  rgba(157,213,58,0) 0%,rgba(237,239,228,0) 21%,rgba(243,241,241,1) 22%,rgba(243,241,241,1) 31%,rgba(243,241,241,1) 100%); 
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#009dd53a', endColorstr='#f3f1f1',GradientType=0 ); /* IE6-9 */
}
.bottom-tab  >.tab-item{max-width:100%;}

.bottom-tab  >.tabs-striped .tab-item.tab-item-active, .tabs-striped .tab-item.active, .tabs-striped .tab-item.activated{
	border-style: none !important;
}

.bottom-tab  >.tabs .tab-item .icon{
  background-repeat: no-repeat;
  background-position: 50%;
  background-size:contain;
  margin-top:25px;
}
.bottom-tab  >.tabs .tab-item .icon.home {
