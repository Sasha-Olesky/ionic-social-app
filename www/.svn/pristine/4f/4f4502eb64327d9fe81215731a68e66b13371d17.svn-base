<ion-header-bar class="background-positive">
	<button class="button button-icon ion-ios-arrow-back" ng-click="go_state('tab.account')"></button>
	<h1 class="title account-title" ng-class="marginleft">申请见面</h1>
</ion-header-bar>
<ion-view class="agree">  
  <ion-content>    
	<ion-scroll>
		<ion-item class="item-thumbnail-left" ng-repeat="item in call_items" type="item-text-wrap">
			<img ng-src="{{item.photo}}"/>
			<h2>{{item.name}}</h2>
			<p><span>{{item.age}}岁</span> | <span>{{item.height}}cm</span> | <span>{{item.address}}<span></p>
		</ion-item>
	</ion-scroll>
  </ion-content>
</ion-view>
